@model ChamberChoiceImput
@using (Html.BeginForm())
{
    using (Html.Awe().BeginContext())
    {
        @Html.EditorFor(model => model.Gender)
        @Html.EditorFor(model => model.ChamberType)
        @Html.EditorFor(model => model.Hobbies)
        @Html.EditorFor(model => model.QuantitySeats)
        @Html.EditorFor(model => model.Floor)

        @if (Model.SystemType == ConstansCS.Type.Orphanage)
        {
            @Html.EditorFor(model => model.HasBrather)
            @Html.EditorFor(model => model.QuantityBrather)
        }
    }
}