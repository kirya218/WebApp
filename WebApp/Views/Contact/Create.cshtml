@model ContactAddInput
@using (Html.BeginForm())
{
    using (Html.Awe().BeginContext())
    {
        @Html.EditorFor(model => model.Name)
        @Html.EditorFor(model => model.FullName)
        @Html.EditorFor(model => model.Email)
